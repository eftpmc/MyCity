{
  "expo": {
    "name": "Urba",
    "slug": "Urba",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "mycity",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.arinora.MyCity"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/expo-favicon.png",
      "bundler": "metro",
      "name": "Urba - Urban Environmental Intelligence",
      "shortName": "Urba",
      "description": "Real-time environmental data and disaster monitoring for US cities",
      "startUrl": "/",
      "display": "standalone",
      "themeColor": "#4A90E2",
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "196b45f0-ebbb-47da-9513-bcbac4eee76b"
      }
    }
  }
}
